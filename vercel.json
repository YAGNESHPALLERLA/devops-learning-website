{
  "version": 2,
  "rewrites": [
    {
      "source": "/jobcy",
      "destination": "https://jobcy-job-portal.vercel.app"
    },
    {
      "source": "/jobcy/(.*)",
      "destination": "https://jobcy-job-portal.vercel.app/$1"
    }
  ],
  "headers": [
    {
      "source": "/jobcy/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}